{
  "name": "autodata",
  "version": "0.9.7",
  "main": "dist/autodata.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist ./docs",
    "build:dev": "babel-node ./node_modules/.bin/webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=prod babel-node ./node_modules/.bin/webpack --config webpack.config.js",
    "build:doc": "gitbook build ./gitbook ./docs",
    "start": "LOCAL=true babel-node ./node_modules/.bin/webpack-dev-server --host 0.0.0.0 --config webpack.config.js",
    "doc": "gitbook serve ./gitbook ./docs",
    "lint": "eslint src",
    "test": "mocha -C -r ./test/setup.js --compilers js:babel-core/register \"src/**/*.spec.js\"",
    "precommit": "yarn run lint",
    "prepush": "yarn test",
    "preversion": "make prepare",
    "version": "generate-changelog && git add -A ./dist ./docs ./CHANGELOG.md",
    "postversion": "git push --no-verify && git push --no-verify --tags"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.2.1",
    "generate-changelog": "^1.5.0",
    "gitbook-cli": "^2.3.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jsdom": "^11.1.0",
    "mocha": "^3.5.0",
    "react-helmet": "^5.1.3",
    "rimraf": "^2.6.1",
    "sinon": "^3.2.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "debounce": "^1.0.2",
    "delegate": "^3.1.3",
    "lodash": "^4.17.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "webpack-bundle-analyzer": "^2.9.0"
  }
}
