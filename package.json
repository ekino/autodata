{
  "name": "autodata",
  "version": "1.0.0",
  "main": "dist/autotrack.js",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist ./doc",
    "build:dev": "babel-node ./node_modules/.bin/webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=prod babel-node ./node_modules/.bin/webpack --config webpack.config.js",
    "build:doc": "gitbook build ./gitbook ./docs",
    "start": "LOCAL=true babel-node ./node_modules/.bin/webpack-dev-server --host 0.0.0.0 --config webpack.config.js",
    "doc": "gitbook serve ./gitbook ./docs",
    "lint": "eslint src",
    "test": "mocha -C -r ./test/setup.js --compilers js:babel-core/register \"src/**/*.spec.js\"",
    "precommit": "yarn run lint",
    "prepush": "yarn test",
    "preversion": "make build && git add -A ./dist ./doc"
  },
  "engines": {
    "node": ">=6.9.0 <7.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.0",
    "eslint-plugin-react": "^7.1.0",
    "gitbook-cli": "^2.3.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "jsdom": "^9.11.0",
    "mocha": "^3.2.0",
    "react-helmet": "^5.1.3",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.16.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "debounce": "^1.0.0",
    "delegate": "^3.1.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
